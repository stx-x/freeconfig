{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "address": "tls://8.8.8.8"
      }
    ]
  },
  "inbounds": [
    {
      "type": "hysteria2",
      "listen": "::",
      "listen_port": 443,
      "users": [
        {
          "password": "RRRRRRRRRRRRRRRRRRRRRRRR" // 第1处修改，修改完请删除注释 `openssl rand -base64 16`
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.de",  // 第2处修改，修改完请删除注释。这是你服务器域名
        "acme": {
          "domain": "example.de", // 第2处修改，修改完请删除注释。这是你服务器域名
          "email": "example@gmail.com" // 第3处修改，修改完请删除注释。随便一个邮箱
        }
      }
    },
    {
      "type": "shadowsocks",
      "listen": "::",
      "listen_port": 8388,
      "network": "tcp",
      "method": "2022-blake3-aes-128-gcm",
      "password": "RRRRRRRRRRRRRRRRRRRRRRRR", // 第5处修改，修改完请删除注释 `openssl rand -base64 16`
      "multiplex": {
        "enabled": true
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "action": "hijack-dns"
      }
    ]
  }
}
